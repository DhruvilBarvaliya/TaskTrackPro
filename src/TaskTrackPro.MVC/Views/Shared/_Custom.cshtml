    <!--// Notification Offcanvas start-->
    <div class="offcanvas offcanvas-start" tabindex="-1" id="offcanvasExample" aria-labelledby="offcanvasExampleLabel">
        <div class="offcanvas-header">
            <h3 class="offcanvas-title" id="offcanvasExampleLabel">Notifications</h3>
            <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>
        <div class="offcanvas-body">
             <div class="accordion" id="notificationAccordion">
                <!-- Unread Notifications -->
                <h4 class="text-danger">Unread Notifications</h4>
                <div class="accordion-item notification unread" id="notif-1">
                    <h2 class="accordion-header">
                        <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapse1">
                            ðŸ”” New Message from John
                        </button>
                    </h2>
                    <div id="collapse1" class="accordion-collapse collapse" data-bs-parent="#notificationAccordion">
                        <div class="accordion-body">
                            John sent you a message: "Hey, are we still on for the meeting at 3 PM?"
                        </div>
                    </div>
                </div>
                <div class="accordion-item notification unread" id="notif-2">
                    <h2 class="accordion-header">
                        <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapse2">
                            ðŸ”” Payment Received
                        </button>
                    </h2>
                    <div id="collapse2" class="accordion-collapse collapse" data-bs-parent="#notificationAccordion">
                        <div class="accordion-body">
                            Your payment of $500 has been successfully received.
                        </div>
                    </div>
                </div>

                <!-- Read Notifications -->
                <h4 class="text-success mt-3">Read Notifications</h4>
                <div class="accordion-item notification read">
                    <h2 class="accordion-header">
                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapse3">
                            âœ… Order Shipped
                        </button>
                    </h2>
                    <div id="collapse3" class="accordion-collapse collapse" data-bs-parent="#notificationAccordion">
                        <div class="accordion-body">
                            Your order #12345 has been shipped and is on the way.
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--// Notification Offcanvas start-->

    <script>
        // Mark notification as read when clicked
        $(".accordion-item.unread").on("click", function () {
            $(this).removeClass("unread").addClass("read");
            $(this).find(".accordion-button").addClass("collapsed");
        });
    </script>
    <!-- Main Contain -->
    <main class="main-contain-wrapper main-content">
        <!-- user profile -->
        <div class="userprofile-menu dropdown-menu">
